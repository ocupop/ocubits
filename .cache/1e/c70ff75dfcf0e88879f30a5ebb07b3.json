ne":1,"column":1439}},{"source":"components/RadioGroup/RadioGroup.jsx","name":"PropTypes","original":{"line":18,"column":8},"generated":{"line":1,"column":1444}},{"source":"components/RadioGroup/RadioGroup.jsx","original":{"line":18,"column":17},"generated":{"line":1,"column":1446}},{"source":"components/RadioGroup/RadioGroup.jsx","name":"instanceOf","original":{"line":18,"column":18},"generated":{"line":1,"column":1454}},{"source":"components/RadioGroup/RadioGroup.jsx","name":"Object","original":{"line":18,"column":29},"generated":{"line":1,"column":1465}},{"source":"components/RadioGroup/RadioGroup.jsx","name":"RadioGroup","original":{"line":20,"column":0},"generated":{"line":1,"column":1474}},{"source":"components/RadioGroup/RadioGroup.jsx","name":"defaultProps","original":{"line":20,"column":11},"generated":{"line":1,"column":1476}},{"source":"components/RadioGroup/RadioGroup.jsx","original":{"line":20,"column":26},"generated":{"line":1,"column":1489}},{"source":"components/RadioGroup/RadioGroup.jsx","name":"disabled","original":{"line":21,"column":2},"generated":{"line":1,"column":1490}},{"source":"components/RadioGroup/RadioGroup.jsx","original":{"line":21,"column":12},"generated":{"line":1,"column":1500}},{"source":"components/RadioGroup/RadioGroup.jsx","name":"required","original":{"line":22,"column":2},"generated":{"line":1,"column":1502}},{"source":"components/RadioGroup/RadioGroup.jsx","original":{"line":22,"column":12},"generated":{"line":1,"column":1512}}],"sources":{"components/RadioGroup/RadioGroup.jsx":"import React from 'react'\nimport PropTypes from 'prop-types'\nimport { getIn, Field, FormikProvider } from 'formik'\nimport Label from '../Label/Label'\nimport RadioInput from '../RadioInput/RadioInput'\nimport './RadioGroup.css'\n// ----------------------------------------------------------------------\n\nRadioGroup.propTypes = {\n  className: PropTypes.string,\n  tooltip: PropTypes.string,\n  hint: PropTypes.string,\n  label: PropTypes.string,\n  required: PropTypes.bool,\n  disabled: PropTypes.bool,\n  radios: PropTypes.array,\n  field: PropTypes.instanceOf(Object),\n  form: PropTypes.instanceOf(Object)\n}\nRadioGroup.defaultProps = {\n  disabled: false,\n  required: false\n}\n\nexport default function RadioGroup ({\n  className = '',\n  label,\n  tooltip,\n  hint,\n  field,\n  required,\n  disabled,\n  radios,\n  form,\n  form: { errors, touched }\n}) {\n  const status = getIn(touched, field.name) && getIn(errors, field.name) ? 'invalid' : ''\n  return (\n    <div className={`ocufield ocu-radiogroup ${className} ${status}`}>\n      <Label label={label} tooltip={tooltip} htmlFor={field.name} required={required} className=\"radioGroupLabel\"/>\n      <div role=\"group\" aria-labelledby=\"my-radio-group\">\n        <FormikProvider value={form}>\n          {radios.map((radio, i) => (\n            <Field\n              key={`radio-${i}`}\n              component={RadioInput}\n              name={field.name}\n              label={radio.label}\n              value={radio.value}\n              hint={radio.hint}\n              tooltip={radio.tooltip}\n              className={radio.className}\n              disabled={disabled || radio.disabled}\n            />\n          ))}\n        </FormikProvider>\n      </div>\n\n      {hint && <div className='hint'>{hint}</div>}\n      {getIn(touched, field.name) && getIn(errors, field.name) && (\n        <small className=\"form-validation-error\">{getIn(errors, field.name)}</small>\n      )}\n    </div>\n  )\n}\n"},"lineCount":null}},"error":null,"hash":"2e796e76708343c544cf199202cea967","cacheData":{"env":{}}}